<?xml version="1.0" encoding="ISO-8859-1"?>
<scene_renderer_commands>
  <!--pre_render-->
  <begin_scene />
  <clear_scene color="true" depth="true" stencil="true" />
  <enable_z_write />
  <enable_z_test />
  <setup_matrices />

  <render_particles />  
  <render_billboards /> 

  <!--Debug-->
  <render_debug_waypoints active="false" />
  <render_debug_process active="true" />
  <render_debug_physics active="false" />
  <render_debug_lights active="false" />
  <render_debug_info grid="true" axis="true"  render_console="true" render_logger="true" render_modifiers="false" render_debug_options="true" render_debug_render="true" />
  <end_scene />
  <present />
</scene_renderer_commands>